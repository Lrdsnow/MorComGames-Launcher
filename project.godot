; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="MorComGamesLauncher"
run/main_scene="res://main.tscn"
boot_splash/image="res://splashscreen.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/macos_native_icon="res://mcgl.icns"
config/windows_native_icon="res://icon.ico"

[autoload]

global="*res://src/global.gd"
usr="*res://src/save.gd"

[display]

window/default_clear_color=Color( 0.180392, 0.180392, 0.180392, 1 )

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 1, 1, 1, 1 )
environment/default_environment="res://default_env.tres"
